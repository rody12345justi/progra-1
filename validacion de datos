// validacion de datos.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;
void main()
{
	int n,p,i;
	p=1;
	do {
	cout<<"Ingrese el valor de n:"<<endl;
    cin>>n;
	} 
	
	while(n<0);
	for (i=1;i<=n;i=i++)
		p=(p*i);
		
	cout<<"El producto es:"<<p<<endl;
		
	getch();
}
